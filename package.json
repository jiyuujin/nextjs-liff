{
  "name": "nextjs",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "HTTPS=true node server.js",
    "build": "next build",
    "export": "next export",
    "start": "next start",
    "format": "prettier --write \"**/*.{js,jsx,json,ts,tsx,md,mdx,css,html,yml,yaml,scss,sass}\" --ignore-path .gitignore",
    "eslint": "eslint ./ --ext ts,tsx",
    "eslint:fix": "eslint ./ --ext ts,tsx --fix",
    "textlint": "textlint ./articles/**",
    "textlint:fix": "textlint ./articles/** --fix",
    "test": "jest",
    "test-cover": "jest --coverage",
    "test:w": "npm run test-cover -- --watch",
    "book": "zenn preview"
  },
  "dependencies": {
    "@line/liff": "2.18.1",
    "firebase": "9.6.1",
    "next": "12.1.4",
    "react": "18.0.0",
    "react-dom": "18.0.0",
    "zenn-cli": "0.1.107"
  },
  "devDependencies": {
    "@babel/core": "^7.17.9",
    "@babel/preset-env": "^7.16.11",
    "@babel/preset-react": "^7.16.7",
    "@nekohack/eslint-config-react": "^0.5.0",
    "@types/enzyme": "^3.10.12",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.23",
    "@types/react": "^18.0.1",
    "@types/react-dom": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.18.0",
    "@typescript-eslint/parser": "^5.18.0",
    "@wojtekmaj/enzyme-adapter-react-17": "^0.6.5",
    "autoprefixer": "^10.4.1",
    "enzyme": "^3.11.0",
    "eslint": "^8.13.0",
    "express": "^4.17.2",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "prettier": "^2.3.2",
    "tailwindcss": "^3.0.10",
    "textlint": "^12.1.0",
    "textlint-rule-ja-no-space-around-parentheses": "^2.2.0",
    "textlint-rule-ja-no-space-between-full-width": "^2.2.0",
    "textlint-rule-ja-space-around-code": "^2.2.0",
    "textlint-rule-no-mix-dearu-desumasu": "^5.0.0",
    "textlint-rule-preset-ja-spacing": "^2.2.0",
    "textlint-rule-preset-ja-technical-writing": "^7.0.0",
    "textlint-rule-spellcheck-tech-word": "^5.0.0",
    "ts-jest": "^27.1.4",
    "typescript": "^4.6.3"
  },
  "jest": {
    "globals": {
      "ts-jest": {
        "tsconfig": "tsconfig.test.json"
      }
    },
    "setupFilesAfterEnv": [
      "<rootDir>/jest-setup.ts"
    ],
    "transform": {
      "^.+\\.(ts|tsx)$": "ts-jest"
    },
    "moduleNameMapper": {
      "\\.(jpg|jpeg|png|gif|eot|otf|webp|svg|ttf|woff|woff2|mp4|webm|wav|mp3|m4a|aac|oga)$": "<rootDir>/__mock__/style-mock.js",
      "\\.(css|less)$": "identity-obj-proxy"
    },
    "collectCoverageFrom": [
      "components/**/*.{ts,tsx}"
    ],
    "testMatch": [
      "**/__tests__/*.(ts|tsx)"
    ],
    "testPathIgnorePatterns": [
      "./.next/",
      "./node_modules/"
    ]
  }
}
